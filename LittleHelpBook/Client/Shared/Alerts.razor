@inherits ResizableCachedComponent

@if (Cache?.Alerts?.Count()>0)
{
   
    <RadzenDataList WrapItems="true" AllowPaging="true" Data="@Cache.Alerts" TItem="Alert" PageSize="12">
        <Template Context="alert">
            <AlertCard Alert="alert" />
        </Template>
    </RadzenDataList>

}

@code {

    protected override async Task OnInitializedAsync()
    {
        await Cache.GetAlerts();
    }

}